{
  "name": "openssl-wasm-js",
  "version": "0.1.0",
  "description": "A minimal JavaScript and WebAssembly port of OpenSSL for web browsers",
  "main": "dist/openssl.js",
  "module": "dist/openssl.esm.js",
  "browser": "dist/openssl.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "npm run build",
    "setup": "node scripts/setup.js",
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "node scripts/build-wasm.js",
    "build:js": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "docs": "typedoc --out docs/api src/index.ts",
    "clean": "rimraf dist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/openssl-wasm-js.git"
  },
  "keywords": [
    "openssl",
    "wasm",
    "webassembly",
    "crypto",
    "cryptography",
    "encryption",
    "security",
    "browser"
  ],
  "author": "Alex Scott <unityaisolutions@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/openssl-wasm-js/issues"
  },
  "homepage": "https://github.com/yourusername/openssl-wasm-js#readme",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-wasm": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}
